# directories

OBJ_DIR = $(MENU_DIR)/obj/

# target: dependencies
#	action

all:
	$(Q)cd $(OBJ_DIR) && $(MAKE) && cd ..

clean:
	$(Q)cd $(OBJ_DIR) && $(MAKE) clean && cd ..

